{
  "expo": {
    "name": "athena-rn",
    "slug": "athena-rn",
    "scheme": "athena-rn",
    "version": "0.0.1",
    "orientation": "default",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.jpg",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.punkah.athenarn"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.punkah.athenarn"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
