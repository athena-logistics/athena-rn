{
  "name": "athena-rn",
  "version": "1.7.1",
  "license": "MIT",
  "scripts": {
    "start": "expo start --dev-client",
    "android": "npm run start -- --android",
    "ios": "npm run start -- --ios",
    "web": "npm run start -- --web",
    "generate": "npx graphql-codegen --config codegen.yml",
    "test": "npm run test:types && npm run test:lint && npm run test:format",
    "test:types": "tsc --noEmit",
    "test:lint": "eslint .",
    "test:format": "prettier --check .",
    "format": "prettier --write .",
    "version": "./bin/sync-expo-version.mjs",
    "translations:extract": "formatjs extract ./{components,screens}/**/*.{ts,tsx} --out-file lang/en.json",
    "translations:merge": "jq -s '(.[0] | map_values(.defaultMessage = \"TBD: \" + .defaultMessage)) + .[1]' lang/en.json lang/de.json | sponge lang/de.json",
    "translations:compile": "formatjs compile-folder lang compiled-lang --ast",
    "postinstall": "npm run generate && npm run translations:compile"
  },
  "dependencies": {
    "@apollo/client": "^3.13.8",
    "@expo-google-fonts/open-sans": "^0.3.0",
    "@expo/metro-config": "~0.20.11",
    "@expo/vector-icons": "^14.1.0",
    "@graphql-codegen/cli": "^5.0.5",
    "@openspacelabs/react-native-zoomable-view": "^2.4.2",
    "@react-native-async-storage/async-storage": "2.1.2",
    "@react-navigation/bottom-tabs": "^7.3.10",
    "@react-navigation/drawer": "^7.3.9",
    "@react-navigation/native": "^7.1.6",
    "@react-navigation/native-stack": "^7.3.10",
    "@sentry/react-native": "~6.10.0",
    "@types/react": "~19.0.10",
    "expo": "^53.0.4",
    "expo-application": "~6.1.4",
    "expo-asset": "~11.1.4",
    "expo-camera": "~16.1.6",
    "expo-constants": "~17.1.4",
    "expo-dev-client": "~5.1.7",
    "expo-device": "~7.1.4",
    "expo-font": "~13.3.0",
    "expo-linking": "~7.1.4",
    "expo-localization": "~16.1.5",
    "expo-splash-screen": "~0.30.7",
    "expo-status-bar": "~2.2.3",
    "expo-updates": "~0.28.11",
    "graphql": "^16.11.0",
    "graphql-ws": "^6.0.4",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-intl": "^7.1.11",
    "react-native": "0.79.1",
    "react-native-gesture-handler": "~2.24.0",
    "react-native-reanimated": "~3.17.5",
    "react-native-reanimated-table": "^0.0.2",
    "react-native-safe-area-context": "5.3.0",
    "react-native-screens": "~4.10.0",
    "react-native-sectioned-multi-select": "^0.10.0",
    "react-native-toast-message": "^2.3.0",
    "react-native-web": "^0.20.0",
    "typescript": "~5.8.3"
  },
  "devDependencies": {
    "@babel/core": "^7.27.1",
    "@formatjs/cli": "^6.6.4",
    "@graphql-codegen/typescript": "^4.1.6",
    "@graphql-codegen/typescript-document-nodes": "^4.0.16",
    "@graphql-codegen/typescript-operations": "^4.6.0",
    "@graphql-codegen/typescript-react-apollo": "^4.3.2",
    "@typescript-eslint/eslint-plugin": "^8.31.1",
    "@typescript-eslint/parser": "^8.31.1",
    "babel-plugin-formatjs": "^10.5.37",
    "eslint": "^9.25.1",
    "eslint-plugin-react": "^7.37.5",
    "graphql-tag": "^2.12.6",
    "prettier": "^3.5.3",
    "prop-types": "^15.8.1",
    "typescript-eslint": "^8.31.1"
  },
  "private": true,
  "expo": {
    "doctor": {
      "reactNativeDirectoryCheck": {
        "listUnknownPackages": false
      }
    }
  }
}
